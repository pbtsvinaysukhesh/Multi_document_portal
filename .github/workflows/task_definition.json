{
  "family": "multidocumentportal",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::630627140541:role/multi_doc_portal",
  "taskRoleArn": "arn:aws:iam::630627140541:role/multi_doc_portal",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "documentportal-container",
      "image": "630627140541.dkr.ecr.us-east-1.amazonaws.com/documentportal:latest",
      "cpu": 1024,
      "memory": 8192,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "documentportal-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/multidocumentportal",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}