{
  "family": "documentportal",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::630627140541:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::630627140541:role/ecsTaskExectionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "documentportalliveclass",
      "image": "630627140541.dkr.ecr.ap-south-1.amazonaws.com/documentportalliveclass",
      "cpu": 1024,
      "memory": 8192,
      "essential": true,
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "documentportal-8080-tcp",
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "apikeyliveclass",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:630627140541:secret:apikeyliveclass-dQ4D3p"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportalliveclass",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
